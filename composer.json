{
    "name": "mydaniel/laravel-geolocation",
    "description": "A flexible, multi-driver geolocation package for Laravel.",
    "keywords": [
        "laravel",
        "geoip",
        "geolocation",
        "ip"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Yousefi Far",
            "email": "daniyousefifar@gmail.com",
            "homepage": "https://github.com/daniyousefifar"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "geoip2/geoip2": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "MyDaniel\\GeoLocation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyDaniel\\GeoLocation\\GeoLocationServiceProvider"
            ],
            "aliases": {
                "GeoLocation": "MyDaniel\\GeoLocation\\Facades\\GeoLocation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
